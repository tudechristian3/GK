    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shadow_box">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:scaleType="fitStart"
            android:src="@drawable/emptylogdefault"
            android:id="@+id/guarantorlogo"
            android:padding="10dp"/>

        <TextView
            android:id="@+id/guarantorname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/guarantorlogo"
            android:layout_margin="4dp"
            android:textColor="@color/tabcolor"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textAppearance="@style/roboto.bold"
            android:text="Sample fsdfsdfsdfsdf fsdffsd"
            android:maxLines="2"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:ellipsize="end"/>

        <TextView

            android:id="@+id/guarantoraddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/guarantorname"
            android:layout_margin="4dp"
            android:textSize="14sp"
            android:text="fsdfdsf"
            android:maxLines="2"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:paddingBottom="30dp"
            android:ellipsize="end"
            />


    </RelativeLayout>




</RelativeLayout>