                            android:paddingLeft="10dp"
                            android:paddingRight="20dp"
                            android:text="COC No."
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/txvCocNo"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.5"
                            android:fontFamily="sans-serif"
                            android:gravity="start"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:text=""
                            android:textAppearance="@style/roboto.bold"
                            android:textColor="@color/colorblack"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="15dp"
                    android:text="Payment Details"
                    android:textAppearance="@style/roboto.bold"
                    android:textColor="@color/colortoolbar"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="15dp"
                    android:background="@color/colorwhite"
                    android:orientation="vertical"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/approvalCodeLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp"
                                android:visibility="gone">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="APPROVAL CODE"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvApprovalCode"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="AMOUNT"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvAmount"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearOtherChargesLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="OTHER CHARGES"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvOtherCharge"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearResellerDiscount"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp"
                                android:visibility="gone">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="RESELLER DISCOUNT"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvResellerDiscount"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="AMOUNT PAID"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvAmountPaid"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearAmountTenderedLayout"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="AMOUNT TENDERED"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvAmountTendered"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearChangeLayout"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingBottom="15dp">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="20dp"
                                    android:text="CHANGE"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txvChange"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:fontFamily="sans-serif"
                                    android:gravity="start"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text=""
                                    android:textAppearance="@style/roboto.bold"
                                    android:textColor="@color/colorblack"
                                    android:textSize="16sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/voucher_details_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:focusableInTouchMode="true"
                                android:orientation="vertical">

                                <ProgressBar
                                    android:id="@+id/pbrecyclerProgress"
                                    style="?android:attr/progressBarStyleSmall"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:visibility="gone" />

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/recyclerViewVouchers"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    tools:listitem="@layout/consumed_details_item" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </ScrollView>

    </RelativeLayout>

    <!--Layout for no connection -->
    <RelativeLayout
        android:id="@+id/nointernetconnection"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorwhite"
        android:visibility="gone">


        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="30dp"
            android:src="@drawable/nointernet" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageView1"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:padding="30dp"
            android:text="Opps! It looks like you're not connected. Please check your internet connection."
            android:textSize="16sp" />


        <ImageView
            android:id="@+id/refreshnointernet"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_below="@id/textView2"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:src="@drawable/refresh4" />

        <ImageView
            android:id="@+id/refreshdisabled1"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_below="@id/textView2"
            android:layout_marginTop="30dp"
            android:gravity="center"
            android:src="@drawable/refreshdisabled"
            android:visibility="gone" />


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:paddingBottom="8dp"
            android:src="@drawable/logowatermark" />
    </RelativeLayout>

</LinearLayout>
